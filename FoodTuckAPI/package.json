{
    "name": "foodtruck-api",
    "version": "0.0.1",
    "description": "Refactory of the Restaurant API.",
    "main": "dist",
    "scripts": {
        "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
        "build": "babel src -s -D -d dist --presets es2015,stage-0",
        "start": "NODE_ENV=production pm2 start dist",
        "prestart": "npm run -s build",
        "lint": "eslint src",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 7,
            "sourceType": "module"
        },
        "env": {
            "node": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    },
    "keywords": [],
    "author": "@helladyo <heladiofog@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.2",
        "express": "^4.16.2",
        "express-jwt": "^6.0.0",
        "jsonwebtoken": "^8.2.2",
        "mongoose": "^5.4.19",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^5.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^4.18.1",
        "nodemon": "^1.18.10"
    }
}
